<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="ToggleButton" x:Key="favoritetogglebutton">
        <Setter Property="OverridesDefaultStyle" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToggleButton">
                    <Grid Background="#02FFFFFF">
                        <Viewbox x:Name="viewbox">
                            <Path Fill="{TemplateBinding Background}" Data="F1 M 6.000,0.000 C 5.250,0.000 4.583,0.375 4.167,0.958 C 3.750,0.375 3.083,0.000 2.333,0.000 C 1.042,0.000 0.000,1.042 0.000,2.333 C 0.000,4.167 4.167,7.500 4.167,7.500 C 4.167,7.500 8.333,4.167 8.333,2.333 C 8.333,1.042 7.292,0.000 6.000,0.000 Z"/>
                        </Viewbox>
                        <Viewbox x:Name="viewboxnot">
                            <Path Fill="{TemplateBinding Foreground}" Data="F1 M 6.000,0.000 C 5.250,0.000 4.583,0.375 4.167,0.958 C 3.750,0.375 3.083,0.000 2.333,0.000 C 1.042,0.000 0.000,1.042 0.000,2.333 C 0.000,4.167 4.167,7.500 4.167,7.500 C 4.167,7.500 8.333,4.167 8.333,2.333 C 8.333,1.042 7.292,0.000 6.000,0.000 L 6.000,0.000 Z M 6.000,0.833 C 6.833,0.833 7.500,1.500 7.500,2.333 C 7.500,3.167 5.875,5.000 4.167,6.417 C 2.458,4.958 0.833,3.125 0.833,2.333 C 0.833,1.500 1.500,0.833 2.333,0.833 C 3.333,0.833 4.167,2.083 4.167,2.083 C 4.167,2.083 4.958,0.833 6.000,0.833"/>
                        </Viewbox>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="viewbox" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="viewboxnot" Property="Visibility" Value="Collapsed"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="False">
                            <Setter TargetName="viewbox" Property="Visibility" Value="Collapsed"/>
                            <Setter TargetName="viewboxnot" Property="Visibility" Value="Visible"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>